prefixes:
  owl: http://www.w3.org/2002/07/owl#
  rdf: http://www.w3.org/1999/02/22-rdf-syntax-ns#
  rdfs: http://www.w3.org/2000/01/rdf-schema#
  xsd: http://www.w3.org/2001/XMLSchema#
  xml: http://www.w3.org/XML/1998/namespace
  rr: http://www.w3.org/ns/r2rml#
  rml: http://semweb.mmlab.be/ns/rml#
  ql: http://semweb.mmlab.be/ns/ql#
  map: http://mapping.example.com/
  ma: http://www.w3.org/ns/ma-ont#
  sd: http://www.w3.org/ns/sparql-service-description#
  dc: http://purl.org/dc/elements/1.1/
  foaf: http://xmlns.com/foaf/0.1/
  rev: http://purl.org/stuff/rev#
  v: http://rdf.data-vocabulary.org/#
  schema: http://schema.org/
  gtfs: http://vocab.gtfs.org/terms#
  geo: http://www.w3.org/2003/01/geo/wgs84_pos#
  dct: http://purl.org/dc/terms/
mappings:
  map_agency_000:
    sources:
    - [/home/thomas/data/de-lijn-gtfs/agency.csv~csv]
    s: http://example.com/de-lijn/agency/$(agency_id)
    po:
    - [rdf:type, http://vocab.gtfs.org/terms#Agency]
    - [foaf:page, $(agency_url)]
    - [foaf:name, $(agency_name)]
    - [gtfs:timeZone, $(agency_timezone)]
    - [dct:language, $(agency_lang)]
    - [foaf:phone, $(agency_phone)]
  map_calendar_date_rules_000:
    sources:
    - [/home/thomas/data/de-lijn-gtfs/calendar_dates.csv~csv]
    s: http://example.com/de-lijn/calendar_date_rule/$(service_id)-$(date)
    po:
    - [rdf:type, http://vocab.gtfs.org/terms#CalendarDateRule]
    - [dct:date, $(date), http://www.w3.org/2001/XMLSchema#date]
    - [gtfs:dateAddition, $(exception_type), http://www.w3.org/2001/XMLSchema#boolean]
  map_feed_000:
    sources:
    - [/home/thomas/data/de-lijn-gtfs/feed_info.csv~csv]
    s: http://example.com/de-lijn/feed/$(feed_publisher_name)
    po:
    - [rdf:type, http://vocab.gtfs.org/terms#Feed]
    - [dct:publisher, $(feed_publisher_name)]
    - [foaf:page, $(feed_publisher_url)]
    - [dct:language, $(feed_lang)]
    - [schema:startDate, $(feed_start_date), http://www.w3.org/2001/XMLSchema#date]
    - [schema:endDate, $(feed_end_date), http://www.w3.org/2001/XMLSchema#date]
    - [schema:version, $(feed_version)]
    - [schema:email, $(feed_contact_email)]
  map_routes_000:
    sources:
    - [/home/thomas/data/de-lijn-gtfs/routes.csv~csv]
    s: http://example.com/de-lijn/routes/$(route_id)
    po:
    - [rdf:type, http://vocab.gtfs.org/terms#Route]
    - [gtfs:shortName, $(route_short_name)]
    - [gtfs:longName, $(route_long_name)]
    - [dct:description, $(route_desc)]
    - [gtfs:routeType, 'http://example.com/de-lijn/resource/RouteType/{route_type}']
    - [gtfs:routeUrl, $(route_url)]
    - [gtfs:color, $(route_color)]
    - [gtfs:textColor, $(route_text_color)]
    - p: gtfs:agency
      o:
        mapping: map_agency_000
        condition:
          function: equal
          parameters:
          - [str1, $(agency_id)]
          - [str2, $(agency_id)]
  map_services2_000:
    sources:
    - [/home/thomas/data/de-lijn-gtfs/calendar_dates.csv~csv]
    s: http://example.com/de-lijn/services/$(service_id)
    po:
    - [rdf:type, http://vocab.gtfs.org/terms#Service]
    - p: gtfs:serviceRule
      o:
        mapping: map_calendar_date_rules_000
        condition:
          function: equal
          parameters:
          - [str1, $(service_id)]
          - [str2, $(service_id)]
  map_stops_000:
    sources:
    - [/home/thomas/data/de-lijn-gtfs/stops.csv~csv]
    s: http://example.com/de-lijn/stops/$(stop_id)
    po:
    - [rdf:type, http://vocab.gtfs.org/terms#Stop]
    - [gtfs:code, $(stop_code)]
    - [dct:identifier, $(stop_id)]
    - [foaf:name, $(stop_name)]
    - [dct:description, $(stop_desc)]
    - [geo:lat, $(stop_lat), http://www.w3.org/2001/XMLSchema#double]
    - [geo:long, $(stop_lon), http://www.w3.org/2001/XMLSchema#double]
    - [gtfs:zone, $(zone_id)]
    - [foaf:page, $(stop_url)]
    - [gtfs:locationType, 'http://example.com/de-lijn/resource/LocationType/{location_type}']
    - [gtfs:wheelchairAccessible, 'http://example.com/de-lijn/resource/WheelchairBoardingStatus/{wheelchair_boarding}']
    - p: gtfs:parentStation
      o:
        mapping: map_stops_000
        condition:
          function: equal
          parameters:
          - [str1, $(parent_station)]
          - [str2, $(stop_id)]
  map_stoptimes_000:
    sources:
    - [/home/thomas/data/de-lijn-gtfs/stop_times.csv~csv]
    s: http://example.com/de-lijn/stoptimes/$(trip_id)-$(stop_id)
    po:
    - [rdf:type, http://vocab.gtfs.org/terms#StopTime]
    - [gtfs:arrivalTime, $(arrival_time), http://www.w3.org/2001/XMLSchema#duration]
    - [gtfs:departureTime, $(departure_time), http://www.w3.org/2001/XMLSchema#dateTime]
    - [gtfs:stopSequence, $(stop_sequence), http://www.w3.org/2001/XMLSchema#integer]
    - [gtfs:headsign, $(stop_headsign)]
    - [gtfs:pickupType, 'http://example.com/de-lijn/resource/PickupType/{pickup_type}']
    - [gtfs:dropOffType, 'http://example.com/de-lijn/resource/DropOffType/{drop_off_type}']
    - [gtfs:distanceTraveled, $(shape_dist_traveled)]
    - p: gtfs:trip
      o:
        mapping: map_trips_000
        condition:
          function: equal
          parameters:
          - [str1, $(trip_id)]
          - [str2, $(trip_id)]
    - p: gtfs:stop
      o:
        mapping: map_stops_000
        condition:
          function: equal
          parameters:
          - [str1, $(stop_id)]
          - [str2, $(stop_id)]
  map_trips_000:
    sources:
    - [/home/thomas/data/de-lijn-gtfs/trips.csv~csv]
    s: http://example.com/de-lijn/trips/$(trip_id)
    po:
    - [rdf:type, http://vocab.gtfs.org/terms#Trip]
    - [gtfs:headsign, $(trip_headsign)]
    - [gtfs:shortName, $(trip_short_name)]
    - [gtfs:direction, $(direction_id)]
    - [gtfs:block, $(block_id)]
    - p: gtfs:service
      o:
        mapping: map_services2_000
        condition:
          function: equal
          parameters:
          - [str1, $(service_id)]
          - [str2, $(service_id)]
    - p: gtfs:route
      o:
        mapping: map_routes_000
        condition:
          function: equal
          parameters:
          - [str1, $(route_id)]
          - [str2, $(route_id)]
